;: playback_preload.cpp |> g++ -std=c++14 -fPIC -shared -o playback_preload.so playback_preload.cpp -ldl |> playback_preload.so
;: playback.cpp |> g++ -std=c++14 -O0 -g -Wfatal-errors -fmax-errors=5 -o playback playback.cpp -I. -lglfw -lGL |> playback
: thrash.cpp |> g++ -std=c++14 -O0 -g -Wfatal-errors -fmax-errors=5 -o thrash thrash.cpp -I. -lglfw -lGL |> thrash
